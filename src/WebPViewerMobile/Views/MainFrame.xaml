<Page
    x:Class="WebPViewerMobile.Views.MainFrame"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="using:WebPViewerMobile.Views"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
        <TextBlock x:Uid="MainPageWelcomeTextBlock" />
        <TextBlock x:Uid="NotSupportedAnimatedWebP" Margin="0,0,0,10" />
        <Button Click="OpenFile">
            <StackPanel Orientation="Horizontal">
                <SymbolIcon Margin="0,0,5,0" Symbol="OpenFile" />
                <TextBlock x:Uid="OpenFileButtonText" />
            </StackPanel>
        </Button>
    </StackPanel>
</Page>
